<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>


<Scene xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.polimi.ingsw.view.gui.OnlineOfflineController">
   <root>
      <VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="720.0" prefWidth="1280.0" spacing="50.0">
         <children>
            <Button fx:id="offlineButton" mnemonicParsing="false" onAction="#playOffline" text="Offine" />
            <Button fx:id="onlineButton" mnemonicParsing="false" onAction="#playOnline" text="Online" />
         </children>
      </VBox>
   </root>
</Scene>
